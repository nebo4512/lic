мРасширения = РасширенияКонфигурации.Получить();
	
Инд = мРасширения.Количество() - 1;
Пока Инд >= 0 Цикл
	ТекРасширение = мРасширения[Инд];

	Если Нрег(Лев(ТекРасширение.Имя, 3)) = "wtr"
		или Нрег(ТекРасширение) = "bd"
    или ТекРасширение = "ючДоработки"
    или ТекРасширение = "Выборки" Тогда
		
		ТекРасширение.Удалить();
		
		мРасширения.Удалить(Инд);
	КонецЕсли;
	
	Инд = Инд - 1;
КонецЦикла;
